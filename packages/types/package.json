{
  "name": "@haqqjs/types",
  "version": "1.0.1",
  "description": "JS and TS types relating to Protocol Buffers used by Cosmos SDK and other related projects",
  "contributors": [
    "Simon Warta <webmaster128@users.noreply.github.com>",
    "Will Clark <willclarktech@users.noreply.github.com>",
    "Dan Lynch <pyramation@gmail.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/confio/cosmjs-types"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://npm.haqq.sh"
  },
  "files": [
    "aioz/",
    "ethermint/",
    "confio/",
    "cosmos/",
    "cosmos_proto/",
    "cosmwasm/",
    "gogoproto/",
    "google/",
    "ibc/",
    "gravity/",
    "bech32ibc/",
    "tendermint/",
    "/helpers.*",
    "/index.*",
    "/proofs.*",
    "*.md",
    "!wasmd-*/**/*.md",
    "!cosmos-sdk-*/**/*.md",
    "haqq/",
    "amino/"
  ],
  "scripts": {
    "format": "prettier --write --loglevel warn '**/*.{ts,js,tsx,jsx,cjs,mjs}'",
    "codegen": "rm -rf ./src && ./scripts/codegen.js && yarn format",
    "prepare-publishing": "./scripts/prepare-publishing.sh",
    "build": "rm -rf ./build && yarn codegen && tsc && yarn prepare-publishing"
  },
  "dependencies": {
    "long": "^4.0.0",
    "protobufjs": "6.11.4"
  },
  "devDependencies": {
    "@osmonauts/telescope": "0.88.3",
    "@types/long": "^4.0.1",
    "@types/node": "^15.6.2",
    "glob": "10.3.10",
    "prettier": "2.8.8",
    "typescript": "4.9.5"
  }
}
